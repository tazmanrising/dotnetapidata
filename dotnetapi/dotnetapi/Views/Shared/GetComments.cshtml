@model IEnumerable<dotnetapi.Data.Models.tblComments>

<table id="tblFacultyDetail" class="table table-striped table-bordered">
    <tr>
        <th style="width:60px;">Comments</th>
        <th>Created By</th>
        <th>Created</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Comments</td>
            <td>@item.createdby</td>
            <td>@item.created</td>
        </tr>
    }
</table>

