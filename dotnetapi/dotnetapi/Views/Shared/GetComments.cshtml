@model IEnumerable<dotnetapi.Data.Models.tblComments>

<table id="tblFacultyDetail" class="tableStyle">
    <tr>
        <th class="tableHeader" style="width:60px;">Comments</th>
        <th class="tableHeader">Created</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td class="tableCell">@item.Comments</td>
            <td class="tableCell">@item.created</td>
        </tr>
    }
</table>

